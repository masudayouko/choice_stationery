<div class="row">

	<div class="col-xs-10">

		<div class="row">
			<% @items.each do |item| %>
				<div class="col-xs-4">
					<%= link_to item_path(item.id) do %>
							<%= attachment_image_tag item, :image, :fill, 500, 360, format: "jpeg"%>
					<% end %>
          <p><%= item.user.name %></p>
				</div>
			<% end %>
		</div>
	</div>

</div>